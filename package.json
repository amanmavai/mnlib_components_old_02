{
  "name": "mnlib_components",
  "version": "0.10.2",
  "description": "reusable react components to be used accross all projects",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "style": "./dist/styles.css",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "npm run build:js:cjs && npm run build:js:esm",
    "build:js:cjs": "tsc --project tsconfig-cjs.json",
    "build:js:esm": "tsc --project tsconfig-esm.json",
    "build:css": "npm run generate:css -- --minify",
    "generate:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css",
    "dev:css": "npm run generate:css -- --watch",
    "clean": "rm -rf dist",
    "publish:pkg": "npm run clean && npm run build && npm publish"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^17.0 || ^18.0",
    "react-dom": "^17.0 || ^18.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "daisyui": "^3.9.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "ag-grid-community": "^30.2.0",
    "ag-grid-enterprise": "^30.2.0",
    "ag-grid-react": "^30.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.284.0",
    "tailwind-merge": "^1.14.0"
  }
}
