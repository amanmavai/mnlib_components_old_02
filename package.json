{
  "name": "mnlib_components",
  "version": "0.0.6",
  "description": "reusable react components to be used accross all projects",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:js",
    "build:js": "npm run build:js:cjs && npm run build:js:esm",
    "build:js:cjs": "tsc --project tsconfig-cjs.json",
    "build:js:esm": "tsc --project tsconfig-esm.json",
    "clean": "rm -rf dist",
    "publish:pkg": "npm run clean && npm run build && npm publish"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^17.0 || ^18.0",
    "react-dom": "^17.0 || ^18.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  }
}
